Cell[CellGroupData[{

Cell["\<\
Check that the sum of the coefficients is equal to the number of graphs\
\>", "Subsection",
 CellChangeTimes->{{3.8935206162992983`*^9, 3.89352061908365*^9}, {
  3.893861267622602*^9, 3.893861268992043*^9}, {3.893875826854437*^9, 
  3.893875847558773*^9}},ExpressionUUID->"acab6a80-c488-4eb6-ae0a-\
83ebcbde0010"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"graphCounter", "[", 
    RowBox[{"n_", ",", "a_"}], "]"}], ":=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"a12", "=", 
        RowBox[{"a", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ",", 
       RowBox[{"a13", "=", 
        RowBox[{"a", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ",", 
       RowBox[{"a23", "=", 
        RowBox[{"a", "[", 
         RowBox[{"[", "3", "]"}], "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{"2", " ", "n"}], ",", "a12"}], "]"}], " ", 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a12"}], ",", "a13"}], "]"}], "*", 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{"2", " ", "n"}], ",", "a12"}], "]"}], " ", 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a12"}], ",", "a23"}], "]"}], "*", 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{"2", " ", "n"}], ",", "a13"}], "]"}], " ", 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a13"}], ",", "a23"}], "]"}], "*", 
      RowBox[{"a12", "!"}], "*", 
      RowBox[{"a13", "!"}], "*", 
      RowBox[{"a23", "!"}], "*", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a12", "-", "a13", "-", "1"}], ")"}], 
       "!!"}], "*", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a12", "-", "a23", "-", "1"}], ")"}], 
       "!!"}], "*", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a13", "-", "a23", "-", "1"}], ")"}], 
       "!!"}], "*", 
      RowBox[{"4", "^", "n"}]}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.893875852511918*^9, 3.893875891662611*^9}},
 CellLabel->"In[43]:=",ExpressionUUID->"ecb82e24-be11-42ac-99b9-dda9d7aa35a4"],

