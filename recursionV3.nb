 CellChangeTimes->{{3.893942399035614*^9, 
  3.8939424097439632`*^9}},ExpressionUUID->"1aa7d50c-724f-4433-b665-\
5573e2d0f8bc"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"coef", "[", "n_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "n"}], "-", "1"}], ")"}], "!!"}], "+", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "n"], 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "i"}], "-", "1"}], ")"}], "!!"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "n"}], "-", 
         RowBox[{"2", "i"}], "-", "1"}], ")"}], "!!"}], 
      RowBox[{"Binomial", "[", 
       RowBox[{"n", ",", "i"}], "]"}]}]}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.8939424250720863`*^9, 3.893942513782247*^9}},
