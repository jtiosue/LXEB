Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"graphCounter", "[", 
    RowBox[{"n_", ",", "a_"}], "]"}], ":=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"a12", "=", 
        RowBox[{"a", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ",", 
       RowBox[{"a13", "=", 
        RowBox[{"a", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ",", 
       RowBox[{"a23", "=", 
        RowBox[{"a", "[", 
         RowBox[{"[", "3", "]"}], "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{"2", " ", "n"}], ",", "a12"}], "]"}], " ", 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a12"}], ",", "a13"}], "]"}], "*", 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{"2", " ", "n"}], ",", "a12"}], "]"}], " ", 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a12"}], ",", "a23"}], "]"}], "*", 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{"2", " ", "n"}], ",", "a13"}], "]"}], " ", 
      RowBox[{"Binomial", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a13"}], ",", "a23"}], "]"}], "*", 
      RowBox[{"a12", "!"}], "*", 
      RowBox[{"a13", "!"}], "*", 
      RowBox[{"a23", "!"}], "*", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a12", "-", "a13", "-", "1"}], ")"}], 
       "!!"}], "*", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a12", "-", "a23", "-", "1"}], ")"}], 
       "!!"}], "*", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "n"}], "-", "a13", "-", "a23", "-", "1"}], ")"}], 
       "!!"}], "*", 
      RowBox[{"4", "^", "n"}]}]}], "\[IndentingNewLine]", "]"}]}], 
