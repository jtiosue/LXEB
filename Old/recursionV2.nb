         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"case", ",", 
           RowBox[{"Keys", "[", "caseToP", "]"}]}], "}"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"tup", ",", 
           RowBox[{"compute\[CapitalDelta]AndComb", "[", 
            RowBox[{"n", ",", "case", ",", "a"}], "]"}]}], "}"}]}], 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"tup", "=", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"combinatorial", " ", "factor"}], ",", " ", 
