Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"computeG", "[", 
    RowBox[{"n_", ",", "a_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "val", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"KeyExistsQ", "[", 
         RowBox[{"g", ",", 
          RowBox[{"{", 
           RowBox[{"n", ",", "a"}], "}"}]}], "]"}], ",", 
        RowBox[{"Return", "@", 
         RowBox[{"g", "[", 
          RowBox[{"{", 
           RowBox[{"n", ",", "a"}], "}"}], "]"}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"AnyTrue", "[", 
         RowBox[{"a", ",", 
          RowBox[{
           RowBox[{
            RowBox[{"#", "<", "0"}], "||", 
            RowBox[{"#", ">", 
             RowBox[{"2", "n"}]}]}], "&"}]}], "]"}], ",", 
        RowBox[{"Return", "@", "0"}]}], "]"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"all", " ", "cases"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"val", "=", 
       RowBox[{"Sum", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"caseToLoop", "[", "case", "]"}], 
          RowBox[{"tup", "[", 
           RowBox[{"[", "1", "]"}], "]"}], 
          RowBox[{"computeG", "[", 
           RowBox[{
            RowBox[{"n", "-", "1"}], ",", 
            RowBox[{"a", "+", 
             RowBox[{"tup", "[", 
              RowBox[{"[", "2", "]"}], "]"}]}]}], "]"}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"case", ",", 
           RowBox[{"Keys", "[", "caseToP", "]"}]}], "}"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"tup", ",", 
           RowBox[{"compute\[CapitalDelta]AndComb", "[", 
            RowBox[{"n", ",", "case", ",", "a"}], "]"}]}], "}"}]}], 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"tup", "=", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"combinatorial", " ", "factor"}], ",", " ", 
            "\[CapitalDelta]"}], "}"}]}], " ", "*)"}], "\[IndentingNewLine]", 
        "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"AppendTo", "[", 
       RowBox[{"g", ",", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"n", ",", "a"}], "}"}], "\[Rule]", "val"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "@", "val"}], ";"}]}], "\[IndentingNewLine]", "]"}]}],
